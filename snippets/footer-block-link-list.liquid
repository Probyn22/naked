<ul class="footer_menu" {{ block.shopify_attributes }}>
  {%- assign link_list = linklists[block.settings.link_list] -%}
  {%- if link_list and link_list.links.size > 0 -%}
    {%- comment -%}
    <h6>{{ link_list.title | escape }}</h6>
    {%- endcomment -%}
    
      {%- for link in link_list.links -%}
       <li> <a href="{{ link.url }}">{{ link.title | escape }}</a></li>
      {%- endfor -%}
  {%- endif -%}
</ul>